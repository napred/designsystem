{
  "name": "root",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow check",
    "lint": "eslint packages/**/src stories",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "test:watch": "jest --watch --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "stories/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.3",
    "@storybook/addon-actions": "^4.0.0-alpha.17",
    "@storybook/addon-knobs": "^4.0.0-alpha.17",
    "@storybook/addon-links": "^4.0.0-alpha.17",
    "@storybook/addon-storysource": "^4.0.0-alpha.17",
    "@storybook/addon-viewport": "^4.0.0-alpha.17",
    "@storybook/addons": "^4.0.0-alpha.17",
    "@storybook/react": "^4.0.0-alpha.17",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.6",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.76.0",
    "flow-copy-source": "^2.0.2",
    "husky": "^1.1.4",
    "jest": "^23.5.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-node": "^23.4.0",
    "jest-image-snapshot": "^2.5.0",
    "jest-styled-components": "^6.1.1",
    "lerna": "^3.4.3",
    "lint-staged": "^7.2.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.1",
    "puppeteer": "^1.1.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.0",
    "react-values": "^0.3.0",
    "rimraf": "^2.6.2",
    "styled-components": "^3.3.3"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "styled-components": "^3.3.3"
  },
  "dependencies": {
    "babel-jest": "^23.4.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "react-display-name": "^0.2.4",
    "react-render-html": "^0.6.0",
    "react-transition-group": "^2.4.0"
